?L7:	NEXT? I >I /?L1
?L3:	PUTB P-XOBJS,0,N
	ZERO? N /TRUE
	EQUAL? N,1 \?L13
	GET P-XOBJS,1 >N
	JUMP ?L14
?L13:	SET 'N,MANY-OBJECTS
?L14:	CALL SET-PRONOUNS,N,P-XOBJS >STACK
	RSTACK

